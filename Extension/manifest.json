{
  "manifest_version": 2,

  "name": "Master Password Chrome Extension",
  "description": "This extension uses Master Password algorithm to automatically login",
  "version": "1.0",

  "icons": {
    "16":  "icon16.png",
    "48":  "icon48.png",
    "128": "icon128.png"
  },

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
